    "babel-register": "6.9.0",
    "eslint": "2.10.2",
    "eslint-config-airbnb": "9.0.1",
    "eslint-plugin-import": "1.11.0",
    "eslint-plugin-jsx-a11y": "1.2.2",
    "eslint-plugin-react": "5.2.2",
    "expect": "1.20.2",
    "jsdom": "9.4.1",
    "mocha": "2.5.3",
    "rimraf": "2.5.3"
  },
